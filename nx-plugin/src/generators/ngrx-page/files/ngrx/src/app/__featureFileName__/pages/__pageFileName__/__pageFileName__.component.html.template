<ocx-portal-page 
  *ngrxLet="viewModel$; let vm"
  role="main"
  <% if(!standalone){ %> permission="<%= pageConstantName %>#VIEW" <% } %>
  helpArticleId="PAGE_<%= featureConstantName %>_<%= pageConstantName %>" 
  [attr.aria-label]="'<%= pageConstantName %>.HEADER' | translate"
>
  <ocx-page-header
    header="<%= pageTitle %>"
    subheader="Created by OneCX App Generator"
  >
  </ocx-page-header>

  <ocx-page-content>
    <h1><%= pageTitle %></h1>
  </ocx-page-content>
</ocx-portal-page>
