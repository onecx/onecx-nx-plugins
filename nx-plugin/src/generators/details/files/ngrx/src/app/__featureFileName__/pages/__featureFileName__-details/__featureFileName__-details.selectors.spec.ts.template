import { select<%= featureClassName %>DetailsViewModel } from './<%= featureFileName %>-details.selectors';

describe('<%= featureClassName %>Details selectors', () => {
  describe('select<%= featureClassName %>DetailsViewModel', () => {
    it('should combine the input to be the viewmodel', () => {
      const details = { id: '10' };
      const detailsLoadingIndicator = true;
      const backNavigationPossible = true;
      const detailsLoaded = true;
      const editMode = true;
      const isSubmitting = true;
      expect(
        select<%= featureClassName %>DetailsViewModel.projector(
          details,
          detailsLoadingIndicator,
          backNavigationPossible,
          detailsLoaded,
          <%_ if(editMode){ _%>
          editMode,
          isSubmitting
          <%_ } _%>
        )
      ).toEqual({
        details,
        detailsLoadingIndicator,
        backNavigationPossible,
        detailsLoaded,
        <%_ if(editMode){ _%>
        editMode,
        isSubmitting
        <%_ } _%>
      });
    });
  });
});
